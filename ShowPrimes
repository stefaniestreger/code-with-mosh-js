// Create a function that passes a limit
// For the limit, display all prime numbers
// Prime number does not have remainder of 0

showPrime(89);

function showPrime(limit) {
    for (let num = 2; num <= limit; num++)       
        if (isPrime(num)) console.log(num);
}

function isPrime(number) {
    for (let factor = 2; factor < number; factor++ )
        if (number % factor === 0)
            return false;

return true; 
}
