// Create blog post object with the following properties
// title, body, author, views, comments, isLive (T/F)
// Comments property has properties author, body
// Create using object literal syntax

let blogPost = {
    title: 'a',
    body: 'b',
    author: 'c',
    views: 10,
    comments: [
        { author: 'd', body: 'e', }
    ],
    isLive: true
};

console.log(blogPost);
